{
  "name": "ponticlaro/bebop-intercom",
  "type": "wordpress-plugin",
  "description": "WordPress Plugin to integrate Intercom for logged in users",
  "require": {
    "composer/installers": "~1.0",
    "ponticlaro/bebop-scripts-loader": "^1",
    "ponticlaro/bebop-http-client": "^1"
  },
  "require-dev": {
    "apigen/apigen": "^4"
  },
  "autoload": {
    "psr-4": {
      "Ponticlaro\\Bebop\\Intercom\\": "lib/"
    }
  },
  "scripts": {
    "generate-docs": "apigen generate --config ./apigen.yaml"
  }
}
